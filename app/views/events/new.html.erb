<%= form_with scope: :event, url: events_path, local: true do |form| %>
    <%= render partial: 'layouts/components/eventPartial', locals: {form: form} %>
    <br/>
    <div class="row justify-content-center">
        <%= form.submit 'Create', {:class => 'btn btn-outline-primary col-lg-2'}%>
    </div>
<% end %>